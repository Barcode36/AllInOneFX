<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <classPathEntry location="mysql-connector-java-5.1.45-bin.jar" />

  <context id="SakilaTables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/sakila"
        userId="root"
        password="">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.allinonefx.model" targetProject="\datos\NetBeansProjects\JavaFX-Examples\AllInOneFX\src\main\java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mybatis.mapper"  targetProject="\datos\NetBeansProjects\JavaFX-Examples\AllInOneFX\src\main\resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>
    
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.allinonefx.dao"  targetProject="\datos\NetBeansProjects\JavaFX-Examples\AllInOneFX\src\main\java">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

<!--
    <table schema="sakila" tableName="film" domainObjectName="Film">
      <property name="useActualColumnNames" value="true"/>
      <generatedKey column="film_id" sqlStatement="DB2" identity="true" />
    </table>
